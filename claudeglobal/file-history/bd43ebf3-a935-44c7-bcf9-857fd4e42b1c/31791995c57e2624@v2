using FractalDataWorks.Collections;

namespace FractalDataWorks.Services.DomainName.Abstractions.Commands;

/// <summary>
/// Base class for DomainName command type options.
/// </summary>
public abstract class DomainNameCommandBase : TypeOptionBase<DomainNameCommandBase>, IDomainNameCommand
{
    /// <summary>
    /// Initializes a new instance of the <see cref="DomainNameCommandBase"/> class.
    /// </summary>
    /// <param name="id">The unique identifier for this command type.</param>
    /// <param name="name">The name of this command type.</param>
    protected DomainNameCommandBase(int id, string name)
        : base(id, name)
    {
    }
}
