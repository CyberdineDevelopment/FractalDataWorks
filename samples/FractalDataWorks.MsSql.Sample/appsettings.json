{
  "ConnectionStrings": {
    "FractalSample": "Server=localhost;Database=FractalSample;Integrated Security=true;TrustServerCertificate=true;Encrypt=false"
  },
  "MsSqlConfiguration": {
    "ConnectionString": "Server=localhost;Database=FractalSample;Integrated Security=true;TrustServerCertificate=true;Encrypt=false",
    "CommandTimeoutSeconds": 30,
    "ConnectionTimeoutSeconds": 15,
    "DefaultSchema": "dbo",
    "SchemaMappings": {
      "Customers": "crm.Customers",
      "Products": "inventory.Products",
      "Categories": "inventory.Categories",
      "Orders": "sales.Orders",
      "OrderItems": "sales.OrderItems"
    },
    "EnableConnectionPooling": true,
    "MinPoolSize": 0,
    "MaxPoolSize": 100,
    "EnableRetryLogic": true,
    "MaxRetryAttempts": 3,
    "RetryDelayMilliseconds": 1000,
    "EnableSqlLogging": true,
    "MaxSqlLogLength": 1000,
    "UseTransactions": false,
    "TransactionIsolationLevel": "ReadCommitted"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "FractalDataWorks": "Debug"
    }
  }
}
