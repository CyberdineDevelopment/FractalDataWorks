{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FastEndpoints": "Information"
    }
  },
  "AllowedHosts": "*",
//#if (DatabaseProvider != "None")
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=FractalDataWorksApiHostDb;Trusted_Connection=true;MultipleActiveResultSets=true"
  },
//#endif
//#if (AuthenticationType == "JWT")
  "Authentication": {
    "JWT": {
      "Issuer": "FractalDataWorksApiHost",
      "Audience": "FractalDataWorksApiHost",
      "SecretKey": "your-very-secure-secret-key-at-least-32-characters-long",
      "ExpirationMinutes": 60
    }
  },
//#endif
//#if (AuthenticationType == "ApiKey")
  "Authentication": {
    "ApiKey": {
      "Key": "your-secure-api-key-here"
    }
  },
//#endif
//#if (EnableCORS)
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://localhost:3001"
    ]
  },
//#endif
  "FractalDataWorks": {
    "Application": {
      "Name": "FractalDataWorksApiHost",
      "Version": "1.0.0",
      "Environment": "Development"
    }
  }
}